#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'xu4fan==0.1','console_scripts','xu4fan'
__requires__ = 'xu4fan==0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('xu4fan==0.1', 'console_scripts', 'xu4fan')()
    )
